{{#if (eq grouping.faces.length 0)}}
  Done!
{{else}}
  {{#each grouping.faces as |face|}}
    {{smiley-face
      face=face
      onSmileyClick=(action "smileyFaceClicked")
    }}
  {{/each}}
  <Input
    class="numerical-input"
    @value={{valueString}}
    @readonly={{isValueReadOnly}}
  />
{{/if}}
